{
  "compilerOptions":{

      "jsx": "react-jsx",
      "allowJs": true,
      "outDir": "./dist", // Add this line
      "noImplicitAny": false,
      "target": "es2016",                                 
      "module": "commonjs",         
      "esModuleInterop": true,      
      "forceConsistentCasingInFileNames": true, 
      "strict": false,             
      "skipLibCheck": true,
      "types":[
          "googlemaps",
          "node"
      ] 
  }
}